div.pull-right
  .row
    button.btn.btn-default.btn-circle(type='button', data-toggle='tooltip', title='بسته', ng-click='backclick()')
      span.glyphicon.glyphicon-remove
  .row 
    a.btn.btn-primary.btn-circle(data-toggle='tooltip', title='درخواست', ng-click='setpageid(1)', href='#contractprimary', ng-class='pageid===1 ? "btn-toggle" : ""')
      span.glyphicon.glyphicon-pencil
  .row(ng-show='requestLevel>1 || (requestLevel===1 && isCreator)')
    a.btn.btn-info.btn-circle(data-toggle='tooltip', title='پیوست ها', ng-click='setpageid(2)', href='#attachments', ng-class='pageid===2 ? "btn-toggle" : ""')
      span.glyphicon.glyphicon-paperclip
  .row(ng-show='requestLevel>1 || (requestLevel===1 && isCreator)')
    a.btn.btn-success.btn-circle(data-toggle='tooltip', title='تصویر قرارداد', ng-click='printbtnclick(); setpageid(3)', href='#printpreview', ng-class='pageid===3 ? "btn-toggle" : ""')
      span.glyphicon.glyphicon-print
script.
    $(function () {
        $('[data-toggle="tooltip"]').tooltip()
    });
